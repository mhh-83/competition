; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="اسوه"
run/main_scene="res://scenes/load.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://sprite/icon.png"

[autoload]

UpdateData="*res://script/UpdateData.gd"
Exit="*res://script/exit.gd"
AddBee="*res://script/add_bee.gd"
ChangeScene="*res://scenes/change_scene.tscn"
Sound="*res://script/sound.gd"

[display]

window/size/viewport_width=1000
window/size/viewport_height=2000
window/stretch/mode="canvas_items"

[input_devices]

pointing/emulate_touch_from_mouse=true
